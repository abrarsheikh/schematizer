                "offset": {
                    "description": "Represents the last known position that has been refreshed.",
                    "type": "integer"
                },
                "status": {
                    "description": "Refresh status to be set.",
                    "enum": [
                        "NOT_STARTED",
                        "IN_PROGRESS",
